{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": "airbnb-base",
  "overrides": [
  ],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "max-len": [2, 80, 2, {
      "ignoreUrls": false,
      "ignoreComments": false
    }]
  },
  "ignorePatterns": ["**/blocks/filteringposts/**/*"]
}
